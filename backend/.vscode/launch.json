  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "DEBUG GAMERES BACKEND",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/build/index.js",
      "preLaunchTask":"tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/build/**/*.js"
      ],
      "env": {
        "CLIENT_ID": "YOUR CLIENT ID HERE",
        "CLIENT_SECRET":"YOUR SECRET HERE"
      }
    }
  ]
}